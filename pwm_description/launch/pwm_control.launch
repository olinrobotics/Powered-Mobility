<?xml version="1.0"?>
<launch>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam command="load" file="$(find pwm_description)/config/pwm_control.yaml"/>
  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner"
	  output="screen" ns="/pwm" args="joint_state_controller velocity_controller lidar_controller"/>
</launch>
