<?xml version="1.0"?>
<launch>
	<arg name="can" default="false"/>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam command="load" file="$(find pwm_description)/config/pwm_control.yaml"/>
  <!-- load the controllers -->

  <node unless="$(arg can)" name="controller_spawner" pkg="controller_manager" type="spawner"
	  output="screen" ns="/wheelchair" args="joint_state_controller can_controller"/>
</launch>
